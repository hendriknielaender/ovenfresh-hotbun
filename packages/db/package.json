{
  "name": "@acme/db",
  "private": true,
  "version": "0.1.0",
  "exports": {
    ".": "./index.ts"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "db:generate": "bun with-env drizzle-kit generate:sqlite",
    "db:push": "bun with-env drizzle-kit push:sqlite",
    "studio": "bun with-env drizzle-kit studio --port 5556",
    "format": "prisma format && prettier --check \"**/*.{mjs,ts,json}\"",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env.local --"
  },
  "dependencies": {
    "drizzle-orm": "^0.28.3",
    "nanoid": "^4.0.2"
  },
  "devDependencies": {
    "@acme/eslint-config": "link:@acme/eslint-config",
    "@acme/prettier-config": "link:@acme/prettier-config",
    "@acme/tsconfig": "link:@acme/tsconfig",
    "dotenv-cli": "^7.2.1",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.47.0",
    "prettier": "^3.0.2",
    "typescript": "^5.1.6"
  },
  "eslintConfig": {
    "extends": [
      "@acme/eslint-config/base"
    ],
    "rules": {
      "@typescript-eslint/consistent-type-definitions": "off"
    }
  },
  "prettier": "@acme/prettier-config"
}
